{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)\\.js$",
        "dest": "/$1.js"
      },
      {
        "src": "/(.*)\\.css$",
        "dest": "/$1.css"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  